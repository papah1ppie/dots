#!/usr/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'todo-cli==1.0.1','console_scripts','todo'
__requires__ = 'todo-cli==1.0.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('todo-cli==1.0.1', 'console_scripts', 'todo')()
    )
